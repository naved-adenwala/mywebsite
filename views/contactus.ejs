<%- include('headerfooter/header'); -%>





 <br>
 <br>
 <br>



 <div class="container">

    <h1 class="bol">Contact Us</h1>

     <form  method="post">
     <div class="col-lg-6">

     
     <div class="row">
        <div class="mb-3">
            <label for="formGroupExampleInput" class="form-label"><span class="sta">*</span> Full Name</label>
            <input type="text" class="form-control" id="formGroupExampleInput" placeholder="Enter Your Name" name="name" required>
          </div>
          <div class="mb-3">
            <label for="formGroupExampleInput" class="form-label"><span class="sta">*</span> Age</label>
            <input type="text" class="form-control" id="formGroupExampleInput" placeholder="Enter Your Age" name="age" required>
          </div>
          <div class="mb-3">
            <label for="formGroupExampleInput" class="form-label"><span class="sta">*</span> Phone</label>
            <input type="text" class="form-control" id="formGroupExampleInput" placeholder="Enter your phone number" name="phone" required>
          </div>
        <div class="mb-3">
            <label for="exampleFormControlInput1" class="form-label" ><span class="sta">*</span> Email address</label>
            <input type="email" class="form-control" id="exampleFormControlInput1" name="email" placeholder="Enter your Email address" required>
          </div>
     </div>
    </div>

<div class="row">
    <label for="formGroupExampleInput" class="form-label" ><span class="sta">*</span> Gender</label>
    <div class="form-check col-lg-2 inp">
        
        <input class="form-check-input" type="radio"   id="flexRadioDefault1" name="gender" value="Male" required>
        <label class="form-check-label" for="flexRadioDefault1">
          Male
        </label>
      </div>
      <div class="form-check col-lg-2 inp">
        <input class="form-check-input" type="radio"  id="flexRadioDefault2"  name="gender" value="Female" required>
        <label class="form-check-label" for="flexRadioDefault2">
         Female
        </label>
      </div>
    </div>
   <div class="col-6">
    <div class="mb-3">
        <label for="exampleFormControlTextarea1" class="form-label" ><span class="sta">*</span> Message</label>
        <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" name="message" required></textarea>
      </div>
   </div>
   

    <div class="col-6 mt">
        <button class="btn btn-primary" type="submit">Submit</button>
      </div>

</form>
 </div>


 

<%- include('headerfooter/footer'); -%>